    script(src='./EmployeePerformance/web/javascripts/kendo.web.min.js')
    script(src='./EmployeePerformance/web/javascripts/kendo.culture.zh-CN.min.js')
    script(src='./EmployeePerformance/web/javascripts/LanguageService.js')
    script(src='./EmployeePerformance/web/javascripts/Bandex.js')
    script(src='./EmployeePerformance/web/javascripts/angular.min.js')
    script(src='./EmployeePerformance/web/javascripts/moment.js')
