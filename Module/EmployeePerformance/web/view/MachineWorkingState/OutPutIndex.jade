doctype html
html(lang='en')
    head
        meta(charset='UTF-8')
        title
        block headcss
            link(href='./EmployeePerformance/web/css/bootstrap.min.css', rel='stylesheet',type='text/css')
            link(type='text/css',href='./EmployeePerformance/web/css/Bandex.css',rel='stylesheet')
            link(type='text/css',href='./EmployeePerformance/web/css/kendo.common.min.css',rel='stylesheet')
            link(type='text/css',href='./EmployeePerformance/web/css/kendo.default.min.css',rel='stylesheet')
    body(style="font-family: Helvetica Neue,Microsoft Yahei,Hiragino Sans GB,WenQuanYi Micro Hei,sans-serif !important;padding: 20px !important;")
        div(ng-app='app', ng-controller='appCtrl')
            label(style='font-size:18px;') [运行状态分析]
            #grid
        script#RATE-template(type='text/x-kendo-template').
            <div style="width: 100%; height: 30px; border: 1px solid \\#009736;">
            <div style="width: #: RATE #%; background-color: \\#009736; height: 30px; "></div>
            </div>
        script#RATE-template1(type='text/x-kendo-template').
            <span>#: RATE #%</span>
