extends ../../../../../views/layout
append headcss 
  link(href='./javascripts/UI/Scripts/media/css/inbox.css', rel='stylesheet', type='text/css')
  style(type='text/css').
    .span {
        float: left !important;
        margin-left: 0px !important;
        width: 300px !important;
        }
        .BZcontrols {
        margin-left: 130px;
        }
        .BZcontrols > .radio, .BZcontrols > .checkbox {
        display: inline-block;
        margin: 8px 15px 0 0 !important;
        padding: 0 !important;
        }
        .inbox-nav li {
        float: right;
        }
        .inbox-nav li a.btn {
        border-radius: 0px !important;
    }

append headjavascript 
    script(type='text/javascript',src='./Efficiencystatistics/web/controller/machineactivationController.js')
    script(src='./javascripts/UI/Scripts/Highcharts-4.0.3/js/highcharts.js', type='text/javascript')
    script(src='./javascripts/UI/Scripts/Highcharts-4.0.3/js/highcharts-3d.js', type='text/javascript')
    script(src='./javascripts/UI/Scripts/Highcharts-4.0.3/js/modules/exporting.js', type='text/javascript')
    script(src='./javascripts/UI/Scripts/BandexHighCharts.js', type='text/javascript')
    script(src='./javascripts/UI/Scripts/Bandex.js', type='text/javascript')
    script(src='./javascripts/UI/Scripts/underscore-min.js', type='text/javascript')

block content
 .row-fluid
  .span12
    h3.page-title(data-lang='AccountManage')
      | #{lang.Efficiencystatistics.EfficiencyStatistical}
    ul.breadcrumb
      li
        i.icon-home
        a(href='/') #{lang.EmployeePerformance.Home}
        i.icon-angle-right
      li
        a(href='javascript:;', data-lang='System') #{lang.Efficiencystatistics.EfficiencyStatistical}
        i.icon-angle-right
      li
        a(href='javascript:;', data-lang='AccountManage') #{lang.Efficiencystatistics.RateOGrainOrMove}
 form.form-horizontal(style='padding: 5px 2px 0px; border: 1px solid rgb(238, 238, 238);')
  .row-fluid
    .span
      .control-group(style='margin-bottom: 5px;')
        label.control-label(data-lang='Common.PASSWORD') #{lang.EmployeePerformance.StartTime}
        .BZcontrols
          input#startTime.m-wrap(type='text', name='startTime')
    .span
      .control-group(style='margin-bottom: 5px;')
        label.control-label(data-lang='Common.PASSWORD') #{lang.EmployeePerformance.EndTime}
        .BZcontrols
          input#endTime.m-wrap(type='text', name='endTime')
  .row-fluid
    .span
      .control-group(style='margin-bottom: 5px;')
        label.control-label(data-lang='Common.PASSWORD') #{lang.Report.TypesOfQueries}
        .BZcontrols(style='line-height: 32px;')
          label.radio(style='margin-top: 0px !important;font-size: 12px;')
            input(type='radio', name='searchType', checked='', value='1')
            | #{lang.EmployeePerformance.Equipment}
          label.radio(style='margin-top: 0px !important;font-size: 12px;')
            input(type='radio', name='searchType', value='2')
            | #{lang.Efficiencystatistics.EquipmentGroup}
    .span
      .control-group(style='margin-bottom: 5px;')
        label.control-label(data-lang='Common.PASSWORD') #{lang.EmployeePerformance.StatisticalMethods}
        .BZcontrols
          input#totalType.m-wrap(type='text', name='totalType')
    div(style='float: left;margin-left:0px;width:350px;')
      .control-group(style='margin-bottom: 5px;')
        label#groupOrMachine_text.control-label(data-lang='Common.PASSWORD') #{lang.EmployeePerformance.Equipment}
        .BZcontrols
          input#groupOrMachine.m-wrap(type='text', name='groupOrMachine')
    .span(style='width: 130px !important;')
      .control-group(style='margin-bottom: 5px;')
        #search.btn.green(style='margin-left: 50px;')
          | #{lang.EmployeePerformance.Search}
          i.icon-search
    .span
      .control-group(style='margin-bottom: 5px;')
        #output.btn.green(style='margin-left: 20px;')
          | #{lang.EmployeePerformance.ExportHTML}
          i.icon-share-alt
 .row-fluid1.inbox
   div
    ul.grouptype.inbox-nav.margin-bottom-10(style='position: absolute;right: 20px;margin-top: 20px;')
      li.active
        a.btn(data-title='1', href='javascript:;') #{lang.EmployeePerformance.Equipment}
        b
      li
        a.btn(data-title='2', href='javascript:;') #{lang.EmployeePerformance.Date}
        b
 #hisChart(style='margin-top: 50px;')